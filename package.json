{
  "name": "react-ssr-template",
  "version": "1.0.0",
  "description": "a simple react server-side rendering template",
  "main": "",
  "scripts": {
    "build-dev:client": "cross-env NODE_ENV=development webpack --config ./config/webpack.client.js",
    "build-dev:server": "cross-env NODE_ENV=development webpack --config ./config/webpack.server.js",
    "server-dev": "node dist/server.js",
    "start-dev": "run-s build-dev:client build-dev:server server-dev",
    "build-prod:client": "cross-env NODE_ENV=production webpack --config ./config/webpack.client.js",
    "build-prod:server": "cross-env NODE_ENV=production webpack --config ./config/webpack.server.js",
    "build": "run-s build-prod:client build-prod:server",
    "start": "node dist/server.js"
  },
  "keywords": [
    "server-side",
    "rendering",
    "ssr",
    "react",
    "boilerplate",
    "template"
  ],
  "author": "ridoan saleh nasution",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@loadable/babel-plugin": "^5.10.3",
    "@loadable/webpack-plugin": "^5.7.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "core-js": "^3.2.1",
    "cross-env": "^6.0.0",
    "css-loader": "^3.2.0",
    "favicons-webpack-plugin": "^1.0.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-manifest-plugin": "^2.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.1",
    "@apollo/react-hooks": "^3.1.1",
    "@apollo/react-ssr": "^3.1.1",
    "@loadable/component": "^5.10.3",
    "@loadable/server": "^5.10.3",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "graphql": "^14.5.8",
    "node-fetch": "^2.6.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0"
  }
}
